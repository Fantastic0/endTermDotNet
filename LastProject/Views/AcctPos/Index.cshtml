@model IEnumerable<LastProject.Models.AcctPos>

@{
    ViewData["Title"] = "Index";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml"; ;
}

<h2>Index</h2>

<p>
    <a>@Html.ActionLink("Create New", "Create", "AcctPos")</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Balance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Balance)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.AcctPostId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.AcctPostId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.AcctPostId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
