@model IEnumerable<LastProject.Models.Acct>


@{
    Layout = "~/Views/Shared/_Layout.cshtml"; ;
}
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a>@Html.ActionLink("Create New", "Create", "Acct")</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CustId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustCat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AcctCat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OpenDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CustId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CustCat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AcctCat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OpenDate)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.AcctId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.AcctId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.AcctId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
